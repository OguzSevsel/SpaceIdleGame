<ui:UXML xmlns:ui="UnityEngine.UIElements" editor-extension-mode="False">
    <Style src="project://database/Assets/Editor/1-Style/ColonyCreatorWindow.uss?fileID=7433441132597879392&amp;guid=1d030984a5147094ab3ea1a205b8ebf4&amp;type=3#ColonyCreatorWindow" />
    <ui:VisualElement name="Root" style="height: 1080px;">
        <ui:VisualElement style="flex-direction: row; flex-grow: 1;">
            <ui:VisualElement name="MainPanel">
                <ui:VisualElement name="Header" style="height: 10%;">
                    <ui:VisualElement name="ButtonPanel" style="justify-content: space-evenly; flex-direction: row; flex-grow: 1;">
