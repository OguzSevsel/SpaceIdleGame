                                <ui:Button text="Create" name="ColonySOCreateButton" class="ButtonTexts Button" />
                                <ui:Button text="Select" name="ColonySOSelectButton" class="ButtonTexts Button" />
                            </ui:VisualElement>
                        </ui:VisualElement>
