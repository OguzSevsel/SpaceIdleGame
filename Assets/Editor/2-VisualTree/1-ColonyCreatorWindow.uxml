                        <ui:Button text="Resource" name="CreateResource" class="ButtonTexts Button menuButtons" />
                        <ui:Button text="Collector SO" name="CreateCollectorSO" class="ButtonTexts Button menuButtons" />
                        <ui:Button text="Collector" name="CreateCollector" class="ButtonTexts Button menuButtons" />
                        <ui:Button text="Colony" name="CreateColony" class="ButtonTexts Button menuButtons" />
                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:VisualElement name="Body" style="min-height: 20px;">
